<?xml version="1.0" encoding="UTF-8"?>
<project name="Jasmine Rhino Envjs Ant" default="run-all" basedir=".">
    <description>Example of using envjs, rhino and Jasmine Reporters together from ant</description>

    <target name="run-all">
        <java jar="lib/js.jar" fork="true">
        	<arg value="-opt"/>
        	<arg value="-1"/>
        	<arg value="lib/envjs.bootstrap.js"/>
			<arg value="SpecRunner.html"/>
        </java>
    </target>

</project>
